{
  "kind": "collectionType",
  "collectionName": "links",
  "info": {
    "singularName": "link",
    "pluralName": "links",
    "displayName": "Link"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "link": {
      "type": "string",
      "regex": "http",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "description": {
      "type": "text"
    }
  }
}
